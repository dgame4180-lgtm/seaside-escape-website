import { useEffect } from "react";
import "./App.css";
import { BrowserRouter, Routes, Route } from "react-router-dom";
import SeasideReward from "./components/SeasideReward";

const Home = () => {
  return <SeasideReward />;
};

function App() {
  return (
    <div className="App">
      <BrowserRouter>
        <Routes>
          <Route path="/" element={<Home />}>
            <Route index element={<Home />} />
          </Route>
        </Routes>
      </BrowserRouter>
    </div>
  );
}

export default App;import React, { useState } from 'react';
import { Button } from './ui/button';
import { Card, CardContent } from './ui/card';
import { Badge } from './ui/badge';
import { Zap, Gift, Anchor, Crown, Heart, Star, Sparkles } from 'lucide-react';
import { mockClaimReward } from '../mock';

const SeasideReward = () => {
  const [isLoading, setIsLoading] = useState(false);
  const [isClaimed, setIsClaimed] = useState(false);

  const handleClaimReward = async () => {
    setIsLoading(true);
    try {
      // Mock claim process
      const result = await mockClaimReward();
      if (result.success) {
        setIsClaimed(true);
        // Redirect to actual claim link after showing success
        setTimeout(() => {
          window.open('https://claimprizenowfree.blogspot.com/?s1=Cip69_Masok', '_blank');
        }, 2000);
      }
    } catch (error) {
      console.error('Claim failed:', error);
    } finally {
      setIsLoading(false);
    }
  };

  return (
    <div className="min-h-screen relative overflow-hidden">
      {/* Background Image */}
      <div 
        className="absolute inset-0 bg-cover bg-center bg-no-repeat"
        style={{
          backgroundImage: `url('https://images.unsplash.com/photo-1603477849227-705c424d1d80?crop=entropy&cs=srgb&fm=jpg&ixid=M3w3NDQ2MzR8MHwxfHNlYXJjaHwxfHx0cm9waWNhbCUyMGJlYWNofGVufDB8fHx8MTc1ODI5ODA2M3ww&ixlib=rb-4.1.0&q=85')`
        }}
      />
      
      {/* Overlay */}
      <div className="absolute inset-0 bg-gradient-to-b from-blue-400/20 via-transparent to-blue-900/30" />
      
      {/* Content */}
      <div className="relative z-10 min-h-screen flex flex-col">
        {/* Header */}
        <header className="py-6 px-4 text-center">
          <div className="flex items-center justify-center space-x-3 mb-4">
            <Anchor className="h-8 w-8 text-blue-600" />
            <h1 className="text-2xl font-bold text-blue-800">Seaside Escape</h1>
            <Anchor className="h-8 w-8 text-blue-600" />
          </div>
        </header>

        {/* Main Content */}
        <main className="flex-1 flex items-center justify-center px-4 py-8">
          <Card className="max-w-2xl w-full bg-white/95 backdrop-blur-md shadow-2xl border-0 rounded-3xl overflow-hidden">
            <CardContent className="p-8 text-center">
              {/* Celebration Icons */}
              <div className="flex justify-center space-x-4 mb-6">
                <div className="animate-bounce">
                  <Crown className="h-12 w-12 text-yellow-500" />
                </div>
                <div className="animate-bounce delay-100">
                  <Gift className="h-12 w-12 text-green-500" />
                </div>
                <div className="animate-bounce delay-200">
                  <Star className="h-12 w-12 text-blue-500" />
                </div>
              </div>

              {/* Title */}
              <h1 className="text-4xl md:text-5xl font-bold text-gray-800 mb-4 leading-tight">
                Congratulations! You Won 
                <span className="text-transparent bg-clip-text bg-gradient-to-r from-yellow-400 to-orange-500">
                  {' '}5000 Free Energy
                </span>
                <br />
                in Seaside Escape!
              </h1>

              {/* Subtitle */}
              <p className="text-xl text-gray-700 mb-8 font-medium">
                Claim Your Energy Reward Now and Continue Your Adventure!
              </p>

              {/* Energy Badge */}
              <div className="flex justify-center mb-8">
                <Badge variant="secondary" className="px-6 py-3 text-xl bg-gradient-to-r from-blue-100 to-green-100 text-blue-800 border-2 border-blue-200">
                  <Zap className="h-6 w-6 mr-2 text-yellow-500" />
                  5000 Free Energy
                  <Sparkles className="h-6 w-6 ml-2 text-yellow-500" />
                </Badge>
              </div>

              {/* Congratulations Message */}
              <div className="bg-gradient-to-r from-blue-50 to-green-50 rounded-2xl p-6 mb-8 border border-blue-200">
                <Heart className="h-8 w-8 text-red-500 mx-auto mb-4" />
                <h3 className="text-2xl font-bold text-gray-800 mb-4">Congratulations on Your Victory!</h3>
                <p className="text-gray-700 text-lg leading-relaxed">
                  You have been selected as a lucky winner! This free energy will help you 
                  explore exotic islands, build your dream resort, and complete exciting missions 
                  in Seaside Escape without limits.
                </p>
              </div>

              {/* Benefits */}
              <div className="grid md:grid-cols-2 gap-4 mb-8">
                <div className="bg-yellow-50 p-4 rounded-xl border border-yellow-200">
                  <Zap className="h-8 w-8 text-yellow-600 mx-auto mb-2" />
                  <h4 className="font-bold text-gray-800 mb-2">Unlimited Energy</h4>
                  <p className="text-gray-600 text-sm">Play longer without waiting for energy refills</p>
                </div>
                <div className="bg-green-50 p-4 rounded-xl border border-green-200">
                  <Gift className="h-8 w-8 text-green-600 mx-auto mb-2" />
                  <h4 className="font-bold text-gray-800 mb-2">Exclusive Rewards</h4>
                  <p className="text-gray-600 text-sm">Access to premium items and decorations</p>
                </div>
              </div>

              {/* Claim Button */}
              {!isClaimed ? (
                <Button 
                  onClick={handleClaimReward}
                  disabled={isLoading}
                  className="w-full md:w-auto px-12 py-4 text-xl font-bold bg-gradient-to-r from-blue-600 to-green-600 hover:from-blue-700 hover:to-green-700 text-white rounded-full shadow-lg hover:shadow-xl transform hover:scale-105 transition-all duration-300"
                >
                  {isLoading ? (
                    <div className="flex items-center">
                      <div className="animate-spin rounded-full h-6 w-6 border-b-2 border-white mr-3"></div>
                      Processing Claim...
                    </div>
                  ) : (
                    <div className="flex items-center">
                      <Gift className="h-6 w-6 mr-3" />
                      Claim Reward Now!
                    </div>
                  )}
                </Button>
              ) : (
                <div className="bg-green-100 p-6 rounded-2xl border-2 border-green-300">
                  <div className="flex items-center justify-center mb-4">
                    <div className="bg-green-500 rounded-full p-3">
                      <Crown className="h-8 w-8 text-white" />
                    </div>
                  </div>
                  <h3 className="text-2xl font-bold text-green-800 mb-2">Reward Successfully Claimed!</h3>
                  <p className="text-green-700">You will be redirected shortly to complete the claim process...</p>
                </div>
              )}

              {/* Footer Note */}
              <p className="text-gray-500 text-sm mt-6">
                * Limited time offer. Claim now to get your 5000 free energy!
              </p>
            </CardContent>
          </Card>
        </main>

        {/* Footer */}
        <footer className="py-6 text-center">
          <p className="text-white/80 text-sm bg-black/20 backdrop-blur-sm rounded-full px-6 py-3 inline-block">
            Â© 2024 Seaside Escape. Enjoy unlimited adventures on tropical islands!
          </p>
        </footer>
      </div>
    </div>
  );
};

export default SeasideReward;/* Seaside Escape Theme Styles */

